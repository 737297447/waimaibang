                    android:layout_width="25dip"
                    android:layout_height="25dip"
                     android:layout_alignBottom="@+id/textView7"
                    android:layout_alignParentRight="true"
                    android:layout_marginBottom="10dip"
                    android:src="@drawable/renwumoban_danxuanborder_normal" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/two_relayout_8"
                android:layout_width="0dip"
                android:layout_height="100dip"
                android:layout_weight="1" >

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="100dip"
                    android:layout_height="75dip"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dip"
                    android:text="浪漫"
                    android:textColor="@color/white"
                    android:textSize="12sp" />

                <ImageView
                    android:id="@+id/two_check_8"
                    android:layout_width="25dip"
                    android:layout_height="25dip"
                     android:layout_alignBottom="@+id/textView8"
                    android:layout_alignParentRight="true"
                    android:layout_marginBottom="10dip"
                    android:src="@drawable/renwumoban_danxuanborder_normal" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/two_relayout_9"
                android:layout_width="0dip"
                android:layout_height="100dip"
                android:layout_weight="1" >

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="100dip"
                    android:layout_height="75dip"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                   />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dip"
                    android:text="户外"
                    android:textColor="@color/white"
                    android:textSize="12sp" />

                <ImageView
                    android:id="@+id/two_check_9"
                    android:layout_width="25dip"
                    android:layout_height="25dip"
                    android:layout_alignBottom="@+id/textView9"
                    android:layout_alignParentRight="true"
                    android:layout_marginBottom="10dip"
                    android:src="@drawable/renwumoban_danxuanborder_normal" />

            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

    <Button
        android:id="@+id/two_next_button"
        android:layout_width="200dip"
        android:layout_height="40dip"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/u24"
        android:gravity="center"
        android:text="下一步"
        android:textColor="@color/black" />

</RelativeLayout>